    <property name="has_separator">False</property>
    <property name="program_name">GDBG</property>
    <property name="copyright" translatable="yes">Copyright 2010 Venugopalan Govindarajan</property>
    <property name="comments" translatable="yes">When printf doesn't cut it... there's gdbg</property>
    <property name="website">http://github.com/gratiscoder/gdbg</property>
    <property name="license" translatable="yes">GPLv2</property>
    <property name="authors">Venugopalan Govindarajan
Sam Post</property>
